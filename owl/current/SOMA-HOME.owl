            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy"/>
            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#definesSetpoint"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relates a disposition to the setpoint parameter defined by the affordance describing the disposition.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>affords setpoint</rdfs:label>
    </owl:ObjectProperty>
    


