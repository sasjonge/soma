        <rdfs:comment>A relation between a Workflow and a Task it contains.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-WF.owl"/>
        <rdfs:label>has step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasSuccedence -->


