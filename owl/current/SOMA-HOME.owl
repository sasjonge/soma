        <rdfs:label>Answer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Antagonist -->


    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Antagonist">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Patient"/>
