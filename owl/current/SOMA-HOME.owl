        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#JointState"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>The position of a joint given in m (prismatic joints) or rad (hinged joints).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has joint position</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointPositionMax -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointPositionMax">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointParameter"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#JointLimit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>The maximum position of a joint given in m (prismatic joints) or rad (hinged joints).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has joint position max</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointPositionMin -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointPositionMin">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointParameter"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#JointLimit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>The minimum position of a joint given in m (prismatic joints) or rad (hinged joints).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has joint position min</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointVelocity -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointVelocity">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointParameter"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#JointState"/>
