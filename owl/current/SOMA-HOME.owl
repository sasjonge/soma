    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isBearerAffordedBy -->


    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isBearerAffordedBy">
