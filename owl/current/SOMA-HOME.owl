    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isDesignOf -->


    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isDesignOf">
