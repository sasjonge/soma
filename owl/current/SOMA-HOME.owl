    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isDefinedInAffordance -->


    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isDefinedInAffordance">
