    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasForceValue -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasForceValue">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegionDataValue"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#ForceAttribute"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#array_double"/>
        <rdfs:comment>A value that quantifies a force given in Newton.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has force value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointEffort -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointEffort">
