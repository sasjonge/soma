        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#JointState"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>The effort applied in a joint given in N (prismatic joint) or N*m (hinged joints).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has joint effort</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointEffortLimit -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointEffortLimit">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointParameter"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl#JointLimit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>The maximum effort applied in a joint given in N (prismatic joint) or N*m (hinged joints).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has joint effort limit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointParameter -->


    <owl:DatatypeProperty rdf:about="http://www.ease-crc.org/ont/SOMA-OBJ.owl#hasJointParameter">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegionDataValue"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Assigns a value for an attribute of a joint.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
        <rdfs:label>has joint parameter</rdfs:label>
